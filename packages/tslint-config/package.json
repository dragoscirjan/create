{
  "name": "@templ-project/tslint-config",
  "version": "0.0.1",
  "description": "Templ Project ESLint Config for TypeScript",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "clear-modules": "rimraf ./node_modules",
    "lint:eslint": "eslint --fix .",
    "lint:oxlint": "oxlint --fix .",
    "lint": "run-s lint:*",
    "prettier": "prettier --config ../../.prettierrc.js --write \"src/**/*.ts\""
  },
  "author": "Dragos Cirjan <dragos.cirjan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@templ-project/eslint-config": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@typescript-eslint/typescript-estree": "^5.57.0"
  }
}
